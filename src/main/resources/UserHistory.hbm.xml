<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.maciej.entity.UserHistory" table="USERHISTORY">
        <id name="id" type="java.lang.Long">
            <column name="ID" />
            <generator class="increment" />
        </id>
        <many-to-one name="user" class="com.maciej.entity.User" not-null="true">
            <column name="USER_ID" />
        </many-to-one>
        <property name="entryTime" type="java.util.Date">
            <column name="ENTRYTIME" />
        </property>
        <property name="entry" type="java.lang.String">
            <column name="ENTRY" />
        </property>
    </class>
</hibernate-mapping>
